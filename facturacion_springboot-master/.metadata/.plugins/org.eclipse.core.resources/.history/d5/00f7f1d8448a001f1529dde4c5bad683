@Service
public class OpenAIService {

    @Value("${openai.api.key}")
    private String apiKey;

    public void someMethod() {
        // Usa la clave API
        System.out.println("Mi clave API es: " + apiKey);
        // Aqu√≠ puedes llamar a OpenAI usando la clave.
    }
}